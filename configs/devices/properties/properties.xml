<CONFIG>
    <RED_SIGNAL SIGNAL_STRENGTH="5" MIN_VALID_INTV="3" BLINKING_THRESHOLD="5" BLINKING_OFF_INTV="30"/>
    <ATCS_SERIAL_CONFIG STATUS="FALSE" COM="Com3" BAUD_RATE="9600" STOP_BIT="0" PARITY_BIT="0"/>
    <MEDIA_SDK NAME="GST" VIDEO_BATCH_SIZE="10" VIDEO_PUSH_TIMEOUT="0.1"/>
    <LOCATION LOC_ID="0120324"/>

    <WEIGHTS>
	    <OBJECT_DETECTOR STATUS="True" TYPE="TENSOR_YOLO" CONFIG_PATH="./configs/devices/properties/tensorRT_yolov4_anpr_new_v2.xml"/>
	<WEATHER_CLASSIFIER STATUS="false" TYPE="YOLO" CONFIG_PATH="./configs/devices/properties/weather.xml"/>
    </WEIGHTS>

    <TRACKER STATUS="true">
        <!--TRACKER_TYPE = 'BOOSTING', 'MIL', 'KCF', 'TLD', 'MEDIANFLOW', 'GOTURN', 'MOSSE', 'CSRT', 'OPTICAL_FLOW'-->
        <PROPERTIES WIDTH="640" HEIGHT="480" NEW_PARAMS="False" TRACKER_TYPE="CSRT"/>
    </TRACKER>

    <TRACKEABLE_OBJECTS STATUS="True" LABELS="LP" VALIDATE="true"/>
    <MATCHEABLE_VEHCILE_WITH_LP STATUS="True" LABELS="LMV,2W,AUTO,HMV,HELMET,NO_HELMET" VALIDATE="False"/>
    <REMOVE_PERSON_FROM_2W STATUS="True" PERSON_LABEL="person" TWO_WHEELER_LABEL="motorbike"/>

    <MISC>
        <!--OUTPUT_VIDEO_BUFFER DURATION IS IN SECONDS-->
        <OUTPUT_TRANSACTION STATUS="true" VIDEO_BUFFER_DURATION="20" FRAME_SKIP_INTERVAL="1" SCENE_SNAPSHOTS="1"/>
        <PROCESSING_THREAD SLEEP="false"/>
        <DEBUG_PARAMS STATUS="False" DURATION="30" DRAW="False" WRITE_FOLDER_DATA="False" RECORD="false"
                      CAMERA_TAMPERING_DRAW="FALSE"
                      FALLEN_OBJECT_DRAW="False" DRAW_TRACKING_OBJECT="true" SLEEP_TIME="0.035"/>
        <HELMET_PARAMS DEBUG_STATUS="False" POST_PROCESSING="TRUE" MATCHING_POINTS="6" VEHICLE_IOU="0.9"
                       INTER_BOXES_IOU="0.5"/>
        <CAMERA_ERROR_HANDLING EOS_RETRY="25" GLOBAL_RETRY="10" ERR_RESET="25" MONITORING_INTV="10"
                               NOTIFICATION_MAX_WAIT_INTV="10"/>
    </MISC>
</CONFIG>
